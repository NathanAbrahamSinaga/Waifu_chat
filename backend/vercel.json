{
    "version": 2,
    "env": {
      "DB_HOST": "@db_host",
      "DB_USER": "@db_user",
      "DB_PASSWORD": "@db_password",
      "DB_NAME": "@db_name",
      "GEMINI_API_KEY": "@gemini_api_key"
    },
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
  